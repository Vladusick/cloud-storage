<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="ru.samoshchenko.cloud.storage.cloudapplication.ChatController">
    <HBox>
        <ListView fx:id="clientView" prefHeight="340.0" prefWidth="216.0"/>
        <ListView fx:id="serverView" prefHeight="340.0" prefWidth="216.0"/>
    </HBox>
    <HBox prefHeight="35.0" prefWidth="432.0">
        <Button onAction="#upload" mnemonicParsing="false" prefHeight="35.0" prefWidth="65.0" text="To server"/>
        <Button onAction="#download" mnemonicParsing="false" prefHeight="41.0" prefWidth="79.0" text="From server"/>
    </HBox>
</VBox>
